# Firebase Cloud Functions Environment Variables
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# Gemini AI API Key for mission briefing generation
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Firebase Project Configuration
# These are typically auto-configured by Firebase, but can be set explicitly if needed
# PROJECT_ID=rapidpro-memphis
# FIREBASE_CONFIG={"projectId":"rapidpro-memphis","storageBucket":"rapidpro-memphis.firebasestorage.app"}
